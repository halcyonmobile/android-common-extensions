        android:text="to_destination_with_uri_and_nav_options_and_extras"
        android:layout_width="match_parent"
        android:layout_height="24dp"/>

    <TextView
        android:id="@+id/to_destination_with_directions"
        android:text="to_destination_with_directions"
        android:layout_width="match_parent"
        android:layout_height="24dp"/>

    <TextView
        android:id="@+id/to_destination_with_directions_and_extras"
        android:text="to_destination_with_directions_and_extras"
        android:layout_width="match_parent"
        android:layout_height="24dp"/>

    <TextView
        android:id="@+id/to_destination_with_directions_and_nav_options"
        android:text="to_destination_with_directions_and_nav_options"
